{
    "name": "leandroperini/dynamo-replicator",
    "description": "A simple copy paste for AWS DynamoDB Table Items for Laravel/Lumen",
    "keywords": ["dynamo", "aws", "lumen", "laravel", "dump", "copy", "import"],
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Leandro Perini",
            "email": "perini105@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "aws/aws-sdk-php": "^3.1"
    },
    "autoload": {
        "psr-4": { "LeandroPerini\\DynamoReplicator\\": "src/" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LeandroPerini\\DynamoReplicator\\DynamoReplicatorServiceProvider"
            ]
        }
}
}
